import java.util.Scanner;
public class Main {	
	public static void main(String[] args) {		
        int n;
        Scanner s = new Scanner(System.in);
        n = s.nextInt();
        int sumo=0, sume=0;
        while (n>0) {
        int r  =n%10;        
        if(r%2==0){
            sume+=r;;
        }
        else if(n%2==1){
            sumo+=r;
        }
        n=n/10;
        }
        System.out.println(sume+" "+sumo);
	}
}
